  "description": "",
  "devDependencies": {
    "@busybox/eslint-config": "5.10.0",
    "@busybox/tsconfig": "1.7.1",
    "@jest/globals": "29.7.0",
    "@nestjs/cli": "10.3.0",
    "@nestjs/schematics": "9.2.0",
    "@nestjs/testing": "8.4.7",
    "@types/aws-lambda": "8.10.131",
    "@types/convict": "6.1.6",
    "@types/express": "4.17.21",
    "@types/luxon": "2.3.2",
    "@types/node": "20.11.6",
    "@types/passport-jwt": "4.0.0",
    "@types/ramda": "0.29.10",
    "@types/supertest": "6.0.2",
    "@types/uuid": "9.0.8",
    "eslint": "8.16.0",
    "graphql-tag": "2.12.6",
    "jest": "28.1.0",
    "jest-environment-node": "29.7.0",
    "jest-mock": "28.1.0",
    "supertest": "6.3.4",
    "ts-jest": "28.0.3",
    "ts-loader": "9.5.1",
    "ts-node": "10.9.2",
    "tsconfig-paths": "4.2.0",
    "typescript": "4.7.2"
  },
  "engines": {
    "node": ">=14",
    "yarn": "Use npm"
  },
  "jest": {
    "collectCoverageFrom": [
      "<rootDir>/**/*.ts",
      "!<rootDir>/**/*.spec.ts",
      "!<rootDir>/migrations/*.ts"
    ],
    "coverageDirectory": "<rootDir>/../coverage",
    "globalSetup": "<rootDir>/test-helpers/jest/e2e-global-setup.js",
    "injectGlobals": false,
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testEnvironment": "<rootDir>/test-helpers/jest/e2e-test-environment.js",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.ts$": "ts-jest"
    }
  },
  "license": "UNLICENSED",
  "name": "backend",
  "private": true,
  "scripts": {
    "build": "nest build",
    "build:lambda": "nest build --webpack -c nest-cli.lambda.json",
    "eslint": "eslint --ext=json,ts,yml",
    "lint": "npx eslint --ext=json,ts,yml --fix .",
    "lint:ci": "npx eslint --ext=json,ts,yml .",
    "migration:create": "npx typeorm migration:create --config ./ormconfig.cli.js",
    "prebuild": "rimraf dist",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:prod": "node dist/main.http.js",
    "test": "jest --maxWorkers=50%",
    "test:ci": "APP_ENV=test NODE_ENV=test Ci=true jest --coverage --ci --runInBand --bail"
  },
  "version": "1.3.0"
}
